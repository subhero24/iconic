{
	"name": "iconic",
	"version": "0.0.1",
	"description": "Iconic icons as react components",
	"main": "build/index.cjs.js",
	"module": "build/index.esm.js",
	"source": "source/index.js",
	"types": "build/index.d.ts",
	"scripts": {
		"lint": "eslint source --max-warnings 0",
		"build_main": "esbuild source/index.js --outfile=build/index.cjs.js --bundle --format=cjs --minify --sourcemap --external:react",
		"build_module": "esbuild source/index.js --outfile=build/index.esm.js --bundle --format=esm --minify --sourcemap --external:react",
		"build": "npm run build_main && npm run build_module && cp source/index.d.ts build/index.d.ts",
		"prepare": "npm run build"
	},
	"keywords": [],
	"author": "Bruno Van Damme",
	"license": "MIT",
	"devDependencies": {
		"@svgr/core": "^6.2.1",
		"camelcase": "^6.3.0",
		"esbuild": "^0.14.19",
		"eslint": "^8.8.0",
		"fs-extra": "^10.0.1",
		"node-fetch": "^3.2.0",
		"prettier": "^2.5.1",
		"zip-unzip-promise": "^2.1.1"
	}
}
